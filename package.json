{
  "name": "nuxt-lazyload-files",
  "version": "1.1.1",
  "description": "Module to allow lazy load specific files, so they're only imported when used (e.g.: screen size related stylings)",
  "homepage": "https://ribeiro-tiago.github.io/nuxt-lazyload-files",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ribeiro-Tiago/nuxt-lazyload-files.git"
  },
  "bugs": {
    "url": "https://github.com/owner/project/issues"
  },
  "keywords": [
    "lazyload",
    "nuxt-module",
    "nuxt3"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "nuxi prepare",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint ./nuxt.config.ts ./src",
    "lint:fix": "bun run eslint --fix",
    "typecheck": "bunx nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "gen:docs": "bunx nuxi generate ./docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.1",
    "cssnano": "^7.0.6",
    "less": "^4.2.2",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "sass": "^1.86.0",
    "stylus": "^0.64.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.1",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.1",
    "@nuxt/test-utils": "^3.17.2",
    "@types/less": "^3.0.8",
    "@types/node": "latest",
    "@types/stylus": "^0.48.43",
    "changelogen": "^0.6.1",
    "eslint": "^9.23.0",
    "nuxt": "^3.16.1",
    "typescript": "~5.8.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "bun@1.2.5"
}
