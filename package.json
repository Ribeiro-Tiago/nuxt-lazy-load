{
  "name": "nuxt-lazyload-files",
  "version": "1.0.0",
  "description": "Module to allow lazy load specific files, so they're only imported when used (e.g.: screen size related stylings)",
  "homepage": "https://ribeiro-tiago.github.io/nuxt-lazyload-files",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ribeiro-Tiago/nuxt-lazy-load"
  },
  "bugs": {
    "url": "https://github.com/owner/project/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "gen:changelog": "changelogen --release",
    "gen:changelog:beta": "changelogen --prerelease",
    "release": "bun run prepack && gen:changelog && npm publish && git push --follow-tags",
    "release:beta": "bun run lint && bun run test && bun run prepack && gen:changelog:beta && npm publish --tag beta && git push --follow-tags",
    "lint": "eslint ./nuxt.config.ts ./src",
    "lint:fix": "bun run eslint --fix",
    "typecheck": "bunx nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0",
    "sass": "^1.86.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.0",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "nuxt": "^3.16.0",
    "typescript": "~5.8.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "bun@1.2.5"
}
